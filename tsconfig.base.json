{
  "compilerOptions": {
    "target": "es2019",
    "module": "ESNext",
    "declaration": true,
    "sourceMap": true,
    "composite": true,
    "strict": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [".git", "node_modules", ".funcignore"]
}